<div class="main-wrapper">
  <div class="floating-img"><img src="https://res.cloudinary.com/dwbun34na/image/upload/v1613324913/better-rent/img4_tbxhyc.jpg" alt=""></div>
  <div class="floating-img-reverse"><img src="https://res.cloudinary.com/dwbun34na/image/upload/v1613324913/better-rent/img2_irgt0h.jpg" alt=""></div>
  <div class="floating-img"><img src="https://res.cloudinary.com/dwbun34na/image/upload/v1613324914/better-rent/img3_kctqcz.jpg" alt=""></div>

  <div class="floating-img-reverse"><img src="https://res.cloudinary.com/dwbun34na/image/upload/v1613324913/better-rent/img1_pzlwlj.jpg" alt=""></div>

  <div class="log-in-card">
    <h1>Start discovering!</h1>
    <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="form-inputs">
        <%= f.input :email,
                    required: false,
                    autofocus: true,
                    input_html: { autocomplete: "email" } %>
        <%= f.input :password,
                    required: false,
                    input_html: { autocomplete: "current-password" } %>
        <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
      </div>

      <div class="form-actions">
        <%= f.button :submit, "Sign in" %>
      </div>
    <% end %>

  <%= render "devise/shared/links" %>
  </div>
  <div class="floating-img-reverse"><img src="https://res.cloudinary.com/dwbun34na/image/upload/v1613325059/better-rent/img5_xpzojd.jpg" alt=""></div>

  <div class="floating-img"><img src="https://res.cloudinary.com/dwbun34na/image/upload/v1613325191/better-rent/img6_xh4jqc.jpg" alt=""></div>
  <div class="floating-img-reverse"><img src="https://res.cloudinary.com/dwbun34na/image/upload/v1613325287/better-rent/img7_qdiuqc.jpg" alt=""></div>
  <div class="floating-img"><img src="https://res.cloudinary.com/dwbun34na/image/upload/v1613325288/better-rent/img8_hunhhu.jpg" alt=""></div>
</div>
