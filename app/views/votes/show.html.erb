
<div class="col-sm-7">
	<div class="rating">
	<h1>show my votes</h1>
		<div class="rating-row">
					<p>Price</p>
					<p><%= @property.rent_per_sqm %> EUR / m²</p>
					<div>
						<% @property.vote.vote_price.to_i.times do %>
							<i class="fas fa-star"></i>
						<% end %>			
					</div>
				</div>

				<div class="rating-row">
					<p>Location</p>
					<p><%= @property.street %> <%= @property.number %></p>
					<div>
						<% @vote.vote_location.to_i.times do %>
							<i class="fas fa-star"></i>
						<% end %>
					</div>
				</div>


				<div class="rating-row">
					<p>Size</p>
					<p><%= @property.lettable_area %> m²</p>
					<div>
						<% @vote.vote_location.to_i.times do %>
							<i class="fas fa-star"></i>
						<% end %>
					</div>
				</div>
		</div>
</div>